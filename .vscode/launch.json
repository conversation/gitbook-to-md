    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Parse Pages",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/node_modules/.bin/ts-node",
        "args": ["${workspaceFolder}/src/parse-pages.ts", "<space>"],
        "env": {
          "API_TOKEN": "<token>"
        }
      }
    ]
  }
  